<!--这个文件中定义的所有变量是与系统相关的-->
<root>
  <!--窗口类型-->
  <windowType>0</windowType>
  <!--窗口宽度-->
  <w>1280</w>
  <!--窗口高度-->
  <h>720</h>
  <!--场景高度-->
  <sceneW>1280</sceneW>
  <!--场景宽度-->
  <sceneH>720</sceneH>
  <!--窗口的原点坐标-->
  <positionX>0</positionX>
  <!--窗口的原点坐标-->
  <positionY>0</positionY>
  <!--是否全屏-->
  <isFullScreen>0</isFullScreen>
  <!--是否隐藏鼠标-->
  <isShowCursor>1</isShowCursor>
  <!--窗口是否拥有边框-->
  <withFrame>1</withFrame>
  <!--窗口模式-->
  <windowMode>4</windowMode>
  <!--窗口标题-->
  <windowTitle>Control</windowTitle>
  <!--窗口的旋转模式-->
  <rotateMode>0</rotateMode>
  <!--窗口的反转模式-->
  <turnOverMode>0</turnOverMode>
  <!--配置的工作模式：0、普通模式，1、服务器模式，2、客户端模式-->
  <CFGMode>0</CFGMode>
  <!--是否开启启动延迟-->
  <isStartDelay>0</isStartDelay>
  <!--启动延迟的时间：单位ms-->
  <startDelayTimer>1000</startDelayTimer>
  <!--是否显示版本号-->
  <isShowVersion>0</isShowVersion>
  <!--版本号-->
  <versionString>1.0.0.0</versionString>
  <!--是否启动定时关机-->
  <isAutoShutDown>0</isAutoShutDown>
  <!--定时关机的时间.小于0表示这项无效-->
  <shutDownTimer_year>2014</shutDownTimer_year>
  <shutDownTimer_month>1</shutDownTimer_month>
  <shutDownTimer_day>23</shutDownTimer_day>
  <shutDownTimer_hour>17</shutDownTimer_hour>
  <shutDownTimer_minute>55</shutDownTimer_minute>
  <shutDownTimer_second>0</shutDownTimer_second>
</root>
